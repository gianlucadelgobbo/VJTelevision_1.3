package FLxER.panels {	import FLxER.comp.ButtonTxt;	public class Mess extends Palette {		var fnz:Function;		public function Mess(w:int,h:int,t:String,tt:String,f:Function=null):void {			super(w,h,t,tt);			fnz = f;			var ok:ButtonTxt = new ButtonTxt(w-51, txt.height+txt.y, 40, 10, "OK", myCancel, null, null);			this.palette.addChild(ok);		}		public function myCancel(e:String):void {			if (fnz is Function) {				fnz()			}			this.parent.removeChild(this)		}	}}